{
  "schemaVersion": 1,
  "id": "coordinatelist",
  "version": "${version}",

  "name": "CList",
  "description": "Coordinate List (CList for short) is a simple minimalistic fabric mod for saving your coordinates. No minimaps. Just a list with in-game waypoints.",
  "authors": [
    "PouekDEV"
  ],
  "contact": {
    "homepage": "https://modrinth.com/mod/clist",
    "sources": "https://github.com/PouekDEV/CList",
    "issues": "https://github.com/PouekDEV/CList/issues"
  },

  "license": "MIT License",
  "icon": "assets/coordinatelist/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "one.pouekdev.coordinatelist.CList"
    ],
    "client": [
      "one.pouekdev.coordinatelist.CListClient"
    ]
  },
  "mixins": [
    "coordinatelist.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.19",
    "fabric-api": "*",
    "minecraft": "1.20.x",
    "java": ">=17",
    "midnightlib": ">=1.1.0"
  },
  "suggests": {
    "another-mod": "*"
  }
}
